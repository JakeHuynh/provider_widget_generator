<idea-plugin>
  <change-notes>
      New release</change-notes>
  <id>huan.huynh.provider_widget_generator</id>
  <name>Provider Widget Generator</name>
  <version>1.0.0</version>
  <vendor email="thanhhuan1990@gmail.com" url="https://github.com/thanhhuan1990">ThanhHuan.VN</vendor>
  <description>
    Provider Widget with Model Template Generator.&lt;br&gt;
    Generates Widget, Model classes.&lt;br&gt;
    &lt;a href="https://github.com/thanhhuan1990/provider_widget_generator"&gt;https://github.com/thanhhuan1990/provider_widget_generator&lt;/a&gt;
    </description>
  <depends>com.intellij.modules.platform</depends>
  <depends>Dart</depends>
  <depends>io.flutter</depends>
  <idea-version since-build="192.6262" until-build="192.*"/>
  <extensions defaultExtensionNs="com.intellij"/>
  <actions>
    <action id="provider_widget_generator.action.GenerateWidgetAction" class="provider_widget_generator.action.GenerateWidgetAction" text="Stateless Widget Generate" description="Generate a new Stateless widget with Provider" icon="/icons/dart_file.svg">
      <add-to-group group-id="NewGroup" anchor="last"/>
    </action>
    <action id="provider_widget_generator.action.GenerateStatefulWidgetAction" class="provider_widget_generator.action.GenerateStatefulWidgetAction" text="Stateful Widget Generate" description="Generate a new Stateful widget with Provider" icon="/icons/dart_file.svg">
      <add-to-group group-id="NewGroup" anchor="last"/>
    </action>
  </actions>
</idea-plugin>
